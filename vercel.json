{
    "rewrites": [
        { "source": "/favicon.png", "destination": "/favicon.png" },
        { "source": "/og-image.jpg", "destination": "/og-image.jpg" },
        { "source": "/og-image.png", "destination": "/og-image.png" },
        { "source": "/(.*)", "destination": "/" }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}